Module
  name: core_lib
  description: Contains core lua classes, functions and constants used by other modules
  author: OTClient team
  website: www.otclient.info
  autoload: true
  autoload-priority: 10
  reloadable: false

  @onLoad: |
    dofiles 'ext'
    dofiles 'math'

    dofile 'const'
    dofile 'util'
    dofile 'globals'
    dofile 'settings'
    dofile 'keyboard'
    dofile 'mouse'

    dofiles 'ui'
    dofiles 'widgets'

