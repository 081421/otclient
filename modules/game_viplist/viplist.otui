VipListLabel < GameLabel
  font: verdana-11px-monochrome
  phantom: false
  &onMousePress: VipList.onVipListLabelMousePress

MiniWindow
  id: vipWindow
  text: VIP List
  height: 100
  icon: viplist.png
  @onClose: VipList.toggle()

  MiniWindowContents
    layout: verticalBox
    anchors.fill: parent
    &onMousePress: VipList.onVipListMousePress
