Module
  name: otclient
  description: Load all other otclient modules
  author: OTClient team
  website: https://github.com/edubart/otclient
  autoLoad: true
  autoLoadPriority: 10
  dependencies:
    - core
    - background
    - topmenu
    - game
    - health_mana
    - inventory
    - skills
    - viplist
    - textmessage
    - chat
    - outfit
    - tibiafiles

  onLoad: |
    require 'otclient'
    return OTClient.init()

  onUnload: |
    OTClient.terminate()
